{
    "version": 2,
    "name": "amit_innovators",
    "builds": [
      {
        "src": "amit_innovators/wsgi.py",
        "use": "@vercel/python",
        "config":{"maxLambdaSize" : "15mb" , "runtime":"python3.12.3"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/python",
        "config":{
            "distDir":"staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "amit_innovators/wsgi.py"
      }
    ]
   
  }
  